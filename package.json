{
  "name": "kiwiirc",
  "version": "1.4.0",
  "description": "Web based IRC client",
  "author": "Darren <darren@darrenwhitlen.com>",
  "scripts": {
    "build": "npm run translate && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "vue-cli-service lint --no-fix",
    "lint:stylelint": "stylelint \"src/**/*.{vue,htm,html,css,sss,less,scss}\"",
    "dev": "npm run translate && vue-cli-service serve",
    "test": "vue-cli-service test:unit",
    "translate": "node build/webpack/locales.js",
    "gendocs": "rm -rf srcdocs/ && jsdoc -c jsdoc.json"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.9.2",
    "@panter/vue-i18next": "^0.15.2",
    "compare-versions": "^3.6.0",
    "css-vars-ponyfill": "^2.2.1",
    "event-emitter": "^0.3.5",
    "font-awesome": "^4.7.0",
    "font-awesome-webpack-4": "^1.0.0",
    "htmlparser2": "^4.1.0",
    "i18next": "^19.4.0",
    "i18next-conv": "^9.2.1",
    "i18next-xhr-backend": "^3.2.2",
    "intersection-observer": "^0.7.0",
    "ip-regex": "^4.1.0",
    "irc-framework": "4.8.1",
    "json5": "^2.1.3",
    "strftime": "^0.10.0",
    "tinycon": "^0.6.8",
    "vue": "^2.6.11",
    "vue-virtual-scroller": "^1.0.4",
    "whatwg-fetch": "^3.0.0",
    "xhr": "^2.5.0"
